{
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "jest": true
      },
      "extends": ["eslint:recommended", "prettier"],
      "plugins": [
        "prettier"
      ],
      "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2020
      },
      "rules": {
        "prettier/prettier": 0,
        "camelcase": [
          "error"
        ],
        "eqeqeq": [
          "error",
          "always"
        ]
      }
    }